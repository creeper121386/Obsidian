   "source": [
    "import Obsidian as ob \n",
    "\n",
    "def cal(x, y):\n",
    "    return (x+y) * x\n",
    "\n",
    "a = ob.Tensor([1, 2, 3])\n",
    "c = ob.Tensor([10, 20, 30])\n",
    "a.need_grad =True\n",
    "c.need_grad =True\n",
    "\n",
    "b = cal(a, c)\n",
    "b.backward()"
   ]
  },
